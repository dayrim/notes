{
  "name": "notes",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "backend:start": "node ./backend/startCompose.cjs",
    "dev": "vite",
    "open-electron": "npx cap open @capacitor-community/electron",
    "copy-electron": "npx cap copy @capacitor-community/electron",
    "sync-electron": "npx cap sync @capacitor-community/electron",
    "open-ios": "npx cap open ios",
    "db:migrate": "node ./db/migrate.cjs",
    "client:generate": "npx electric-sql generate --service http://localhost:5133 --proxy postgresql://prisma:proxy_password@localhost:65432/electric",
    "electron": "run-s build sync-electron copy-electron open-electron",
    "ios": "run-s build sync open-ios",
    "build": "vite build",
    "sync": "npx cap sync",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "ionic:build": "npm run build",
    "ionic:serve": "npm run dev"
  },
  "dependencies": {
    "@capacitor-community/electron": "^5.0.1",
    "@capacitor-community/sqlite": "^5.5.0",
    "@capacitor/android": "^5.4.1",
    "@capacitor/core": "^5.6.0",
    "@capacitor/ios": "5.4.1",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.8",
    "@mui/icons-material": "^5.15.3",
    "@mui/material": "^5.15.3",
    "electric-sql": "^0.9.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reflect-metadata": "^0.2.1",
    "typeorm": "^0.3.19",
    "uuid": "^9.0.1",
    "wa-sqlite": "github:rhashimoto/wa-sqlite"
  },
  "devDependencies": {
    "@capacitor/cli": "^5.6.0",
    "@databases/pg-migrations": "^5.0.2",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jeep-sqlite": "^2.5.6",
    "npm-run-all": "^4.1.5",
    "react-sqlite-hook": "^3.2.1",
    "shelljs": "^0.8.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
